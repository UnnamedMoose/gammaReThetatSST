FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      RASProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

RASModel                        gammaSST;

turbulence                      on;

printCoeffs                     on;

gammaSSTCoeffs
{
    kInf                        0.0;                // freestream values for
    omegaInf                    0.0;                // controlling the decay
                                                    // of the turbulent
                                                    // quantities in the free-
                                                    // stream. If both are 0,
                                                    // freestream decay control
                                                    // is inactive.
}

